        <TextBox x:Name="FridayStart" HorizontalAlignment="Left" Height="21" Margin="118,282,0,-1" TextWrapping="Wrap" VerticalAlignment="Top" Width="115"/>
        <TextBox x:Name="FridayEnd" HorizontalAlignment="Left" Height="21" Margin="238,282,0,-1" TextWrapping="Wrap" VerticalAlignment="Top" Width="116"/>
        <Label x:Name="label_Copy4" Content="Friday" HorizontalAlignment="Left" Height="29" Margin="19,278,0,-5" VerticalAlignment="Top" Width="94"/>

        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="38" Margin="10,314,0,0" Stroke="Black" VerticalAlignment="Top" Width="651"/>
        <TextBox x:Name="SundayStart" HorizontalAlignment="Left" Height="21" Margin="118,76,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="115"/>
        <TextBox x:Name="SundayEnd" HorizontalAlignment="Left" Height="21" Margin="238,76,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="116"/>
