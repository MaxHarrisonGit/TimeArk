        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="38" Margin="10,232,0,0" Stroke="Black" VerticalAlignment="Top" Width="651"/>
        <TextBox x:Name="ThursdayStart" HorizontalAlignment="Left" Height="21" Margin="118,241,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="115"/>
        <TextBox x:Name="ThursdayEnd" HorizontalAlignment="Left" Height="21" Margin="238,241,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="116"/>
        <Label x:Name="label_Copy3" Content="Thursday" HorizontalAlignment="Left" Height="29" Margin="19,237,0,0" VerticalAlignment="Top" Width="94"/>

