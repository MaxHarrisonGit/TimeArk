        <Label x:Name="label" Content="Saturday" HorizontalAlignment="Left" Height="29" Margin="19,319,0,0" VerticalAlignment="Top" Width="94"/>
        <TextBox x:Name="MoProject" HorizontalAlignment="Left" Height="21" Margin="532,118,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="116"/>
        <TextBox x:Name="TuProject" HorizontalAlignment="Left" Height="21" Margin="532,159,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="116"/>
        <TextBox x:Name="WedProject" HorizontalAlignment="Left" Height="21" Margin="532,200,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="116"/>
        <TextBox x:Name="ThProject" HorizontalAlignment="Left" Height="21" Margin="532,241,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="116"/>
        <TextBox x:Name="FrProject" HorizontalAlignment="Left" Height="21" Margin="532,282,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="116"/>
        <TextBox x:Name="SatProject" HorizontalAlignment="Left" Height="21" Margin="532,323,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="116"/>
        <Label x:Name="label1" Content="Start Time (00:00)" HorizontalAlignment="Left" Height="24" Margin="118,43,0,0" VerticalAlignment="Top" Width="115"/>
        <Label x:Name="label1_Copy" Content="End Time (00:00)" HorizontalAlignment="Left" Height="24" Margin="238,43,0,0" VerticalAlignment="Top" Width="115"/>
        <Label x:Name="label1_Copy1" Content="Location" HorizontalAlignment="Left" Height="24" Margin="389,43,0,0" VerticalAlignment="Top" Width="115"/>
        <Label x:Name="label1_Copy2" Content="Project" HorizontalAlignment="Left" Height="28" Margin="533,43,0,0" VerticalAlignment="Top" Width="115"/>
        <Button x:Name="AutoFill" Content="Auto Fill" HorizontalAlignment="Left" Height="22" Margin="606,16,0,0" VerticalAlignment="Top" Width="55" Click="AutoFill_Click"/>
        <Button x:Name="Submit" Content="Submit" HorizontalAlignment="Left" Height="21" Margin="589,372,0,0" VerticalAlignment="Top" Width="72" Click="Submit_Click"/>
        <Button x:Name="Save" Content="Save as Auto Fill" HorizontalAlignment="Left" Height="22" Margin="10,372,0,0" VerticalAlignment="Top" Width="95" Click="Save_Click"/>
        <CheckBox x:Name="UpdateCheck" Content="Keep Current Plan" HorizontalAlignment="Left" Height="18" Margin="130,16,0,0" VerticalAlignment="Top" Width="118"/>


